{
  "expo": {
    "name": "ExpoProj",
    "description": "A very interesting project.",
    "slug": "ExpoProj",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "ios": {
      "bundleIdentifier": "com.code.qr"
    },
    "android": {
      "package": "com.code.qr"
    },
    "platforms": [ "ios", "android" ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}
